{
  "name": "customjs",
  "version": "1.0.21",
  "description": "This is a plugin for Obsidian (https://obsidian.md) that enables custom javascript across mobile and desktop.",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "build-ci": "npm run build -- --failAfterWarnings",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint  '--fix  --ext .ts .'",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Sam Lewis",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.2",
    "obsidian": "^0.12.0",
    "obsidian-dataview": "^0.4.11",
    "prettier": "^3.1.1",
    "rollup": "^2.32.1",
    "rollup-plugin-copy": "^3.4.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "compare-versions": "^3.6.0",
    "debuggable-eval": "^1.0.0"
  }
}